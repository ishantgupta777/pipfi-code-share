{
	"name": "pipfi--code-share-",
	"displayName": "pipfi (code share)",
	"publisher": "ishantgupta777",
	"description": "p.ip.fi extension to share your code with anyone.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:pipfi--code-share-.shareCode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pipfi--code-share-.shareCode",
				"category": "pipfi (share code)",
				"title": "Share code using p.ip.fi"
			},
			{
				"command": "pipfi--code-share-.shareSelectedCode",
				"category": "pipfi (share code)",
				"title": "Share selected code using p.ip.fi"
			}
		],
		"menus": {
      "editor/context": [
        {
          "command": "pipfi--code-share-.shareCode",
          "group": "z_commands"
        },
				{
          "command": "pipfi--code-share-.shareSelectedCode",
          "group": "z_commands"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"form-data": "^4.0.0"
	}
}
